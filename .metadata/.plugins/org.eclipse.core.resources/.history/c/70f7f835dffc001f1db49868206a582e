package fp.universidades.test;

import fp.universidades.tipos.Centro;
import fp.universidades.tipos.Espacio;
import fp.universidades.tipos.TipoEspacio;

public class TestCentro {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Centro c1 = new Centro("Etsii", "Reina mercedes", 4, 1);
		Espacio e1 = new Espacio(TipoEspacio.OTRO,"Sitio donde hay cosas",30 );
		Espacio e2 = new Espacio(TipoEspacio.LAB,"lab",10 );
		Espacio e3 = new Espacio(TipoEspacio.OTRO,"nose cabe",1 );
		
		c1.nuevoEspacio(e1);
		c1.nuevoEspacio(e2);
		c1.nuevoEspacio(e3);
		System.out.println(c1);
		System.out.println(c1.getEspacios());
	}

}
